  "source": "pragma solidity ^0.4.18;\n\ncontract HelloWorld {\n    uint public balance;\n    int public balance2;\n    bool public flag;\n\n    mapping(uint => uint) public names;\n    string public ower;\n    \n    // constructor\n    function HelloWorld(uint _balance) {\n        balance = 0;\n        balance2 = -1;\n        ower = \"Frank\";\n        flag = true;\n\n    }\n\n    function update_balance(uint _balance) {\n        balance = _balance;\n    }\n\n    function update_balance2(int _balance) {\n        balance2 = _balance;\n    }\n\n    function update_names(uint _id, uint _age) {\n        names[_id] = _age;\n    }\n}",
